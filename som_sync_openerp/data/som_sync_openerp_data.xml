<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data noupdate="1">
        <record id="odoo_url_api" model="res.config">
            <field name="name">odoo_url_api</field>
            <field name="value">http://localhost:8069/api/v1/</field>
            <field name="description">URL d'Odoo</field>
        </record>
        <record id="odoo_api_key" model="res.config">
            <field name="name">odoo_api_key</field>
            <field name="value">###</field>
            <field name="description">API key d'Odoo</field>
        </record>
        <record id="odoo_erp_models_to_sync" model="res.config">
            <field name="name">odoo_erp_models_to_sync</field>
            <field name="value">['account.account','res.country','res.country.state','res.municipi','res.partner','res.partner.address','res.partner.bank']</field>
            <field name="description">OpenERP models to sync with Odoo</field>
        </record>
    </data>
</openerp>
